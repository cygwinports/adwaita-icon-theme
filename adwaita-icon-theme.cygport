inherit gnome2

NAME="adwaita-icon-theme"
VERSION=3.18.0
RELEASE=1
CATEGORY="GNOME"
SUMMARY="GNOME desktop icon theme"
DESCRIPTION="GNOME desktop icon theme"
REQUIRES="hicolor-icon-theme"
HOMEPAGE="http://art.gnome.org/"

ARCH=noarch

src_install() {
	cd ${B}
	cyginstall

	> ${D}/usr/share/icons/Adwaita/icon-theme.cache
}
